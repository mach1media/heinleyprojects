{
  "require": {
    "craftcms/cms": "^3.7.26",
    "craftcms/mailgun": "2.0.0",
    "craftcms/redactor": "2.8.8",
    "ether/seo": "3.7.4",
    "hybridinteractive/craft-position-fieldtype": "1.0.17",
    "spicyweb/craft-embedded-assets": "2.10.0",
    "spicyweb/craft-neo": "2.12.2",
    "statikbe/craft-config-values": "1.0.1",
    "verbb/field-manager": "2.2.4",
    "verbb/image-resizer": "2.1.0",
    "verbb/redactor-tweaks": "2.0.4",
    "verbb/super-table": "2.7.1",
    "vlucas/phpdotenv": "^5.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.4"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
